<?xml version="1.0"?>
<configuration>

  <location path="Cart.aspx">
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>

  <location path="ModifyMovies.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
